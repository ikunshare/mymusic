<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.0dip" />
            <gradient android:startColor="#ffe7e7e7" android:endColor="#ffe7e7e7" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffe7e7e7" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <gradient android:startColor="#ff00b4ee" android:endColor="#ff00b4ee" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff00b4ee" />
            </shape>
        </clip>
    </item>
</layer-list>
